{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noFallthroughCasesInSwitch": true,
    "strictNullChecks": true,
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "module": "esnext",
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "es5",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "global-types": [
        "dist/global-types/global-types",
        "dist/global-types"
      ],
      "state-management": [
        "dist/state-management/state-management",
        "dist/state-management"
      ],
      "array-helpers": [
        "dist/array-helpers/array-helpers",
        "dist/array-helpers"
      ],
      "date-time-helpers": [
        "dist/date-time-helpers/date-time-helpers",
        "dist/date-time-helpers"
      ],
      "notification": [
        "dist/notification/notification",
        "dist/notification"
      ],
      "state-db": [
        "dist/state-db/state-db",
        "dist/state-db"
      ],
      "optimistic-http": [
        "dist/optimistic-http/optimistic-http",
        "dist/optimistic-http"
      ],
      "state-sync": [
        "dist/state-sync/state-sync",
        "dist/state-sync"
      ],
      "state-model": [
        "dist/state-model/state-model",
        "dist/state-model"
      ],
      "confirm-dialog": [
        "dist/confirm-dialog/confirm-dialog",
        "dist/confirm-dialog"
      ],
      "dynamic-forms": [
        "dist/dynamic-forms/dynamic-forms",
        "dist/dynamic-forms"
      ],
      "form-sheet": [
        "dist/form-sheet/form-sheet",
        "dist/form-sheet"
      ],
      "model-form": [
        "dist/model-form/model-form",
        "dist/model-form"
      ],
      "state-auth": [
        "dist/state-auth/state-auth",
        "dist/state-auth"
      ],
      "cdk-selectable": [
        "dist/cdk-selectable/cdk-selectable",
        "dist/cdk-selectable"
      ]
    }
  },
  "angularCompilerOptions": {
    "fullTemplateTypeCheck": true,
    "strictInjectionParameters": true
  },
  "typedocOptions": {
    "entryPoints": ["projects/**/src/public-api.ts"],
    "out": "docs"
  }
}