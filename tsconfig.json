{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noFallthroughCasesInSwitch": true,
    "strictNullChecks": true,
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "module": "es2020",
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "es2015",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "global-types": [
        "dist/global-types/global-types",
        "dist/global-types"
      ],
      "state-management": [
        "dist/state-management/state-management",
        "dist/state-management"
      ],
      "array-helpers": [
        "dist/array-helpers/array-helpers",
        "dist/array-helpers"
      ],
      "date-time-helpers": [
        "dist/date-time-helpers/date-time-helpers",
        "dist/date-time-helpers"
      ],
      "notification": [
        "dist/notification/notification",
        "dist/notification"
      ],
      "state-db": [
        "dist/state-db/state-db",
        "dist/state-db"
      ],
      "optimistic-http": [
        "dist/optimistic-http/optimistic-http",
        "dist/optimistic-http"
      ],
      "google-places-autocomplete": [
        "dist/google-places-autocomplete/google-places-autocomplete",
        "dist/google-places-autocomplete"
      ],
      "state-sync": [
        "dist/state-sync/state-sync",
        "dist/state-sync"
      ],
      "confirm-dialog": [
        "dist/confirm-dialog/confirm-dialog",
        "dist/confirm-dialog"
      ],
      "dynamic-forms": [
        "dist/dynamic-forms/dynamic-forms",
        "dist/dynamic-forms"
      ],
      "form-sheet": [
        "dist/form-sheet/form-sheet",
        "dist/form-sheet"
      ],
   
      "state-auth": [
        "dist/state-auth/state-auth",
        "dist/state-auth"
      ],
      "cdk-selectable": [
        "dist/cdk-selectable/cdk-selectable",
        "dist/cdk-selectable"
      ],   
      "model/core": [
        "dist/model/model-core/model-core",
        "dist/model/model-core"
      ],
      "model/state-commands": [
        "dist/model/model-state-commands/model-state-commands",
        "dist/model/model-state-commands"
      ],  
      "model/state-fetcher": [
        "dist/model/model-state-fetcher/model-state-fetcher",
        "dist/model/model-state-fetcher"
      ],
      "model/form": [
        "dist/model/model-form/model-form",
        "dist/model/model-form"
      ],
      "model/data-table": [
        "dist/model/model-data-table/model-data-table",
        "dist/model/model-data-table"
      ],
    }
  },
  "angularCompilerOptions": {
    "fullTemplateTypeCheck": true,
    "strictInjectionParameters": true
  },
  "typedocOptions": {
    "entryPoints": [
      "libs/**/src/public-api.ts"
    ],
    "out": "docs"
  }
}