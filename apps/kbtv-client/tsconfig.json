{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./out-tsc/app",
    "types": [],
    "paths": {
      "@core/*": ["src/app/core/*"],
      "@actions/*": ["src/app/actions/*"],
      "@shared-app/*": ["src/app/shared-app/*"],
      "@shared/*": ["src/app/shared/*"],
      "@shared-timesheet/*": ["src/app/feature-modules/timesheet-modules/shared-timesheet/*"],
      "@shared-mission/*": ["src/app/feature-modules/mission-modules/shared-mission/*"],
      "date-time-helpers": [
        "../../dist/date-time-helpers/date-time-helpers",
        "../../dist/date-time-helpers"
      ],
      "global-types": [
        "../../dist/global-types/global-types",
        "../../dist/global-types"
      ],
      "state-management": [
        "../../dist/state-management/state-management",
        "../../dist/state-management"
      ],
      "array-helpers": [
        "../../dist/array-helpers/array-helpers",
        "../../dist/array-helpers"
      ],
      "notification": [
        "../../dist/notification/notification",
        "../../dist/notification"
      ],
      "state-db": [
        "../../dist/state-db/state-db",
        "../../dist/state-db"
      ],
      "optimistic-http": [
        "../../dist/optimistic-http/optimistic-http",
        "../../dist/optimistic-http"
      ],    
      "state-sync": [
        "../../dist/state-sync/state-sync",
        "../../dist/state-sync"
      ],
      "confirm-dialog": [
        "../../dist/confirm-dialog/confirm-dialog",
        "../../dist/confirm-dialog"
      ],
      "dynamic-forms": [
        "../../dist/dynamic-forms/dynamic-forms",
        "../../dist/dynamic-forms"
      ],
      "form-sheet": [
        "../../dist/form-sheet/form-sheet",
        "../../dist/form-sheet"
      ],
      "state-auth": [
        "../../dist/state-auth/state-auth",
        "../../dist/state-auth"
      ],
      "cdk-selectable": [
        "../../dist/cdk-selectable/cdk-selectable",
        "../../dist/cdk-selectable"
      ],
      "google-places-autocomplete": [
        "../../dist/google-places-autocomplete/google-places-autocomplete",
        "../../dist/google-places-autocomplete"
      ],        
      "model/*": ["../../dist/model/*"],
      "global-utils": [
        "../../dist/global-utils/global-utils",
        "../../dist/global-utils"
      ],
      "mat-dynamic-form-controls": [
        "../../dist/mat-dynamic-form-controls/mat-dynamic-form-controls",
        "../../dist/mat-dynamic-form-controls"
      ],
    },
  },
  "files": [
    "src/main.ts",
    "src/polyfills.ts"
  ],
  "include": [
    "src/**/*.ts"
  ],
  "exclude": [
    "src/test.ts",
    "src/**/*.spec.ts"
  ]
}
