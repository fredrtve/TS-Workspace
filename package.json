{
  "name": "fredtv-workspace",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "buildGit": "ng build  --prod --base-href /kbtvAngularLive/ --stats-json",
    "build": "ng build --project kbtv-client --prod --stats-json",
    "buildPublishGit": "ng build --prod --base-href /kbtvAngularLive/ --stats-json && angular-cli-ghpages --dir dist/kbtv-client --repo=https://github.com/fredtvet/kbtvAngularLive.git",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "analyze:wba": "webpack-bundle-analyzer dist/kbtv-client/stats.json",
    "analyze:sme": "source-map-explorer dist/kbtv-client/**/*.js",
    "compodoc": "npx compodoc -p tsconfig.json",
    "buildAll": "node scripts/build-all.js",
    "buildLibs": "node scripts/build-libs.js",
    "buildDebug": "env NG_BUILD_MANGLE=false NG_BUILD_MINIFY=false NG_BUILD_BEAUTIFY=true ng build --project kbtv-client --prod --stats-json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^12.0.2",
    "@angular/cdk": "^12.0.2",
    "@angular/common": "^12.0.2",
    "@angular/compiler": "^12.0.2",
    "@angular/core": "^12.0.2",
    "@angular/forms": "^12.0.2",
    "@angular/material": "^12.0.2",
    "@angular/platform-browser": "^12.0.2",
    "@angular/platform-browser-dynamic": "^12.0.2",
    "@angular/pwa": "^0.901.15",
    "@angular/router": "^12.0.2",
    "@angular/service-worker": "^12.0.2",
    "ag-grid-angular": "^25.3.0",
    "ag-grid-community": "^25.3.0",
    "idb-keyval": "^3.2.0",
    "rxjs": "~6.5.5",
    "tslib": "^2.1.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^12.0.2",
    "@angular/cli": "^12.0.2",
    "@angular/compiler-cli": "^12.0.2",
    "@angular/language-service": "^12.0.2",
    "@compodoc/compodoc": "^1.1.11",
    "@types/jasmine": "^3.6.7",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^14.14.41",
    "codelyzer": "^5.2.2",
    "dependency-cruiser": "^9.24.0",
    "jasmine-core": "^3.7.1",
    "jasmine-spec-reporter": "^5.0.2",
    "karma": "^6.3.2",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage-istanbul-reporter": "^3.0.3",
    "karma-jasmine": "^3.3.1",
    "karma-jasmine-html-reporter": "^1.5.4",
    "ng-packagr": "^12.0.2",
    "protractor": "^7.0.0",
    "ts-essentials": "^7.0.1",
    "ts-node": "^8.10.2",
    "tslint": "~6.1.0",
    "typedoc": "^0.20.34",
    "typescript": "^4.2.4",
    "webpack-bundle-analyzer": "^3.9.0"
  }
}
