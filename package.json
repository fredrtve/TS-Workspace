{
  "name": "fredtv-workspace",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "buildGit": "ng build  --prod --base-href /kbtvAngularLive/ --stats-json",
    "build": "ng build --project kbtv-client --prod --stats-json",
    "buildPublishGit": "ng build --prod --base-href /kbtvAngularLive/ --stats-json && angular-cli-ghpages --dir dist/kbtv-client --repo=https://github.com/fredtvet/kbtvAngularLive.git",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "analyze:wba": "webpack-bundle-analyzer dist/kbtv-client/stats.json",
    "analyze:sme": "source-map-explorer dist/kbtv-client/**/*.js",
    "compodoc": "npx compodoc -p tsconfig.json",
    "buildAll": "node build-all.js",
    "buildDebug": "env NG_BUILD_MANGLE=false NG_BUILD_MINIFY=false NG_BUILD_BEAUTIFY=true ng build --project kbtv-client --prod --stats-json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^11.2.7",
    "@angular/cdk": "^11.2.6",
    "@angular/common": "^11.2.7",
    "@angular/compiler": "^11.2.7",
    "@angular/core": "^11.2.7",
    "@angular/flex-layout": "^10.0.0-beta.32",
    "@angular/forms": "^11.2.7",
    "@angular/localize": "^11.2.7",
    "@angular/material": "^11.2.6",
    "@angular/platform-browser": "^11.2.7",
    "@angular/platform-browser-dynamic": "^11.2.7",
    "@angular/pwa": "^0.901.15",
    "@angular/router": "^11.2.7",
    "@angular/service-worker": "^11.2.7",
    "ag-grid-angular": "^23.2.1",
    "ag-grid-community": "^23.2.1",
    "hammerjs": "^2.0.8",
    "idb-keyval": "^3.2.0",
    "rxjs": "~6.5.5",
    "tslib": "^2.1.0",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1100.7",
    "@angular/cli": "^11.2.6",
    "@angular/compiler-cli": "^11.2.7",
    "@angular/language-service": "^11.2.7",
    "@compodoc/compodoc": "^1.1.11",
    "@types/jasmine": "^3.6.7",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^14.14.35",
    "codelyzer": "^5.2.2",
    "dependency-cruiser": "^9.24.0",
    "jasmine-core": "^3.7.1",
    "jasmine-spec-reporter": "^5.0.2",
    "karma": "^5.2.3",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage-istanbul-reporter": "^3.0.3",
    "karma-jasmine": "^3.3.1",
    "karma-jasmine-html-reporter": "^1.5.4",
    "ng-packagr": "^11.2.4",
    "protractor": "^7.0.0",
    "ts-node": "^8.10.2",
    "tslint": "~6.1.0",
    "typedoc": "^0.20.34",
    "typescript": "^4.1.5",
    "webpack-bundle-analyzer": "^3.9.0"
  }
}
