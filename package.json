{
  "name": "fredtv-workspace",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --project kbtv-client --configuration production --stats-json",
    "buildDemo": "ng build --project kbtv-client --configuration demo --base-href /demo/ --stats-json",
    "buildTest": "ng build --project kbtv-client --configuration e2e",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "analyze:wba": "webpack-bundle-analyzer dist/kbtv-client-prod/stats.json",
    "analyze:sme": "source-map-explorer dist/kbtv-client-prod/**/*.js",
    "cleanupImports": "tslint --config tslint-imports.json --fix --project .",
    "compodoc": "npx compodoc -p tsconfig.json",
    "buildAll": "node scripts/build-all.js",
    "buildLibs": "node scripts/build-libs.js",
    "generateApiDocs": "node scripts/generate-api-docs.js",
    "buildDebug": "env NG_BUILD_MANGLE=false NG_BUILD_MINIFY=false NG_BUILD_BEAUTIFY=true ng build --project kbtv-client --configuration production --stats-json",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^12.2.1",
    "@angular/cdk": "^12.2.1",
    "@angular/common": "^12.2.1",
    "@angular/compiler": "^12.2.1",
    "@angular/core": "^12.2.1",
    "@angular/forms": "^12.2.1",
    "@angular/google-maps": "^12.0.4",
    "@angular/material": "^12.2.1",
    "@angular/platform-browser": "^12.2.1",
    "@angular/platform-browser-dynamic": "^12.2.1",
    "@angular/pwa": "^0.901.15",
    "@angular/router": "^12.2.1",
    "@angular/service-worker": "^12.2.1",
    "@googlemaps/js-api-loader": "^1.12.1",
    "@microsoft/api-extractor": "^7.18.4",
    "ag-grid-angular": "^25.3.0",
    "ag-grid-community": "^25.3.0",
    "hammerjs": "^2.0.8",
    "idb-keyval": "^3.2.0",
    "karma-coverage": "^2.0.3",
    "rxjs": "~6.5.5",
    "tslib": "^2.1.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^12.2.1",
    "@angular/cli": "^12.2.1",
    "@angular/compiler-cli": "^12.2.1",
    "@angular/language-service": "^12.2.1",
    "@compodoc/compodoc": "^1.1.11",
    "@cypress/schematic": "^1.5.0",
    "@types/jasmine": "^3.6.7",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^14.17.9",
    "codelyzer": "^5.2.2",
    "cypress": "^8.3.0",
    "cypress-file-upload": "^5.0.8",
    "dependency-cruiser": "^9.24.0",
    "jasmine-core": "^3.7.1",
    "jasmine-spec-reporter": "^5.0.2",
    "karma": "^6.3.2",
    "karma-chrome-launcher": "^3.1.0",
    "karma-jasmine": "^3.3.1",
    "karma-jasmine-html-reporter": "^1.5.4",
    "ng-packagr": "^12.0.2",
    "ts-essentials": "^7.0.1",
    "ts-node": "^8.10.2",
    "tslint": "~6.1.0",
    "typedoc": "^0.20.34",
    "typescript": "^4.2.3",
    "webpack-bundle-analyzer": "^3.9.0"
  }
}
